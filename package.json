{
  "name": "@integec/fetch-task",
  "version": "1.0.6",
  "description": "Fetch Task Wrapper",
  "main": "dist/fetch-task.js",
  "scripts": {
    "build": "babel src/index.js --out-file dist/fetch-task.js --source-maps inline",
    "build:watch": "babel src/index.js --out-file dist/fetch-task.js --watch --source-maps inline",
    "build:prod":
      "cross-env NODE_ENV=production babel src/index.js --out-file dist/fetch-task.js --source-maps",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "pretty": "prettier --debug-check \"src/**/*.js\" ",
    "pretty:write": "prettier --write \"src/**/*.js\"",
    "precommit": "lint-staged",
    "lint:fix-file": "eslint --fix",
    "prepublishOnly": "npm run build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:fobbyal/fetch-task.git"
  },
  "keywords": ["fetch", "task", "data.task"],
  "author": "Alex Liang <aliang@integconsulting.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "dependencies": {
    "data.maybe": "^1.2.2",
    "data.task": "^3.1.1",
    "futurize": "^1.2.0",
    "whatwg-fetch": "^2.0.4"
  },
  "peerDependencies": {},
  "lint-staged": {
    "*.{js,json,css,md}": ["npx prettier --write", "git add"]
  }
}
